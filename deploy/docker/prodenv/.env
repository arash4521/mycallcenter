# Hostnames of the services #
#############################
ASTERISK_HOSTNAME=asterisk
KAMAILIO_HOSTNAME=kamailio
OMNILEADS_HOSTNAME=omniapp
REDIS_HOSTNAME=redis
WOMBAT_HOSTNAME=dialer

###########################
# Django default settings #
###########################
DJANGO_SETTINGS_MODULE=ominicontacto.settings.production
PYTHONPATH=/opt/omnileads/ominicontacto

###############################################
# Location of installations inside containers #
###############################################
ASTERISK_LOCATION=
INSTALL_PREFIX=/opt/omnileads/
KAMAILIO_LOCATION=

#############################
# Version of other services #
#############################
DIALER_VERSION=18.08.3-232
POSTGRES_VERSION=11
RTPENGINE_VERSION=5.5.3.1
